    "zod": "^3.23.8",
    "pino": "^9.5.0",
    "@prisma/client": "^5.18.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "tsx": "^4.19.2",
    "@types/node": "^20.14.12",
    "prisma": "^5.18.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
